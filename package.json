{
  "name": "color-harmony-generator",
  "version": "0.0.6",
  "description": "Library for generation of harmonious color themes.",
  "keywords": [
    "color",
    "harmony",
    "generator"
  ],
  "author": {
    "name": "Rostyslav Khanas",
    "url": "https://github.com/rtkhanas"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src",
